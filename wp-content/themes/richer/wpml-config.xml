<wpml-config>
    <custom-fields>
        <custom-field action="copy">richer_titlebar</custom-field>
        <custom-field action="copy">richer_title_aligment</custom-field>
        <custom-field action="translate">richer_title</custom-field>
        <custom-field action="copy">richer_title_color</custom-field>
        <custom-field action="copy">richer_title_bg_color</custom-field>
        <custom-field action="translate">richer_subtitle</custom-field>
        <custom-field action="copy">richer_subtitle_color</custom-field>
        <custom-field action="copy">richer_subtitle_bg_color</custom-field>
        <custom-field action="copy">richer_bg_color</custom-field>
        <custom-field action="copy">richer_bg_opacity</custom-field>
        <custom-field action="copy">richer_bg_image</custom-field>
        <custom-field action="copy">richer_bg_repeat</custom-field>
        <custom-field action="copy">richer_bg_position_x</custom-field>
        <custom-field action="copy">richer_bg_position_y</custom-field>
        <custom-field action="copy">richer_bg_fullscreen</custom-field>
        <custom-field action="copy">richer_bg_fixed</custom-field>
        <custom-field action="copy">richer_border_color</custom-field>
        <custom-field action="copy">richer_titlebar_outer_padding</custom-field>
        <custom-field action="copy">richer_titlebar_inner_padding</custom-field>
        <custom-field action="copy">richer_crumbs_check</custom-field>
        <custom-field action="copy">richer_portfoliofilter</custom-field>
        <custom-field action="copy">richer_show-portfolio-filter</custom-field>
        <custom-field action="ignore">richer_revolutionslider</custom-field>
        <custom-field action="copy">richer_bgurl</custom-field>
        <custom-field action="copy">richer_bgstyle</custom-field>
        <custom-field action="copy">richer_bgcolor</custom-field>
        <custom-field action="copy">richer_bg_position_x</custom-field>
        <custom-field action="copy">richer_bg_position_y</custom-field>
        <custom-field action="copy">richer_bg_fixed</custom-field>
        <custom-field action="copy">richer_portfolio-detaillayout</custom-field>
        <custom-field action="copy">richer_portfolio-details</custom-field>
        <custom-field action="copy">richer_portfolio-relatedposts</custom-field>
        <custom-field action="copy">richer_portfolio-lightbox</custom-field>
        <custom-field action="copy">richer_screenshot</custom-field>
        <custom-field action="copy">richer_gridlayout</custom-field>
        <custom-field action="copy">richer_slider_thumb</custom-field>
        <custom-field action="copy">richer_source</custom-field>
        <custom-field action="copy">richer_embed</custom-field>
        <custom-field action="copy">richer_quote_url</custom-field>
        <custom-field action="copy">richer_post_url</custom-field>
        <custom-field action="copy">richer_audio_embed</custom-field>
        <custom-field action="translate">richer_portfolio-client</custom-field>
        <custom-field action="translate">richer_wysiwyg</custom-field>
        <custom-field action="translate">richer_quote_name</custom-field>
        <custom-field action="translate">richer_testi_caption</custom-field>
        <custom-field action="translate">richer_testi_info</custom-field>
        <custom-field action="copy">richer_testi_url</custom-field>
    </custom-fields>
    <custom-types>
        <custom-type translate="1">portfolio</custom-type>
        <custom-type translate="1">testi</custom-type>
        <custom-type translate="1">wpcf7_contact_form</custom-type>
    </custom-types>    
    <taxonomies>
        <taxonomy translate="1">portfolio_filter</taxonomy>
        <taxonomy translate="1">post_tag</taxonomy>
        <taxonomy translate="1">page_keywords</taxonomy>
    </taxonomies>       
    <language-switcher-settings>
        <key name="icl_lang_sel_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#EEEEEE</key>
        </key>
        <key name="icl_lang_sel_footer_config">
            <key name="font-current-normal">#393939</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#F5F5F5</key>
            <key name="background-current-hover">#FFFFFF</key>
            <key name="font-other-normal">#393939</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#F5F5F5</key>
            <key name="background-other-hover">#FFFFFF</key>
            <key name="border">transparent</key>
            <key name="background">transparent</key>
        </key>    
        <key name="icl_language_switcher_sidebar">1</key>
        <key name="display_ls_in_menu">0</key>
        <key name="icl_language_switcher_sidebar_blog-widgets">0</key>
        <key name="icl_widget_title_show">0</key>
        <key name="icl_lang_sel_type">dropdown</key>
        <key name="icl_lso_link_empty">0</key>
        <key name="icl_lso_flags">1</key>
        <key name="icl_lso_native_lang">1</key>
        <key name="icl_lso_display_lang">0</key>
        <key name="icl_lang_sel_footer">0</key>
        <key name="icl_post_availability">0</key>
        <key name="icl_post_availability_position">below</key>
        <key name="icl_post_availability_text">This post is also available in: %s</key>
        <key name="icl_additional_css">#lang_sel img, #lang_sel_list img, #lang_sel_footer img { top: 0px; display: inline; }</key>
    </language-switcher-settings>    
</wpml-config>


